class PrincipalController < ApplicationController
  def principal
  end

  def revistas
    @unit = "revistas"
    respond_to do |format|
      format.html { redirect_to :catalogo}
    end
  end

  def tesis
    @unit = "tesis"
    respond_to do |format|
      format.html { redirect_to :catalogo}
    end
  end

  def publicaciones
    @unit = "pub-colmex"
    respond_to do |format|
      format.html { redirect_to :catalogo}
    end
  end
end
